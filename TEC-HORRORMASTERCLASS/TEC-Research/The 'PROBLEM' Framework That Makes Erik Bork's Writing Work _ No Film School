.postpage__mainCol .widget__section::before, .postpage__sideCol .one-col .widget__section::before, .postpage-content .all-related-sections a::before, .one-col .all-related-sections a::before { position: absolute; top: 17px; left: 26px; transform: translate(-50%, -50%); pointer-events: none; font-family: var(--icons-font); font-size: 20px; }

.prod-reason::before { position: absolute; content: ""; border-style: solid; border-top-color: rgba(0, 0, 0, 0); border-right-color: rgba(0, 0, 0, 0); border-left-color: rgba(0, 0, 0, 0); border-image: initial; border-width: 0px 0px 33px 10px; right: 100%; bottom: 0px; border-bottom-color: var(--green); display: block; }

