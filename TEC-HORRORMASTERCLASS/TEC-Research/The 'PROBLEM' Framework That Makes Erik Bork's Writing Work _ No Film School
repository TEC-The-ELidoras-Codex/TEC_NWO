.one-col .widget .body { width: 100%; }

.one-col .body-description { position: relative; margin-bottom: 20px; }

.one-col .widget .body .widget__brief { display: none; }

.widget__show-more { font-weight: var(--btn-weight); background-color: var(--btn-bg); color: var(--btn-color); font-family: var(--btn-family); border: solid 1px var(--btn-bg); order: 3; margin: 25px auto; font-size: 16px; line-height: 19px; border-radius: 23px; padding: 14px 29px 11px; width: fit-content; transition: 0.35s; }

.widget__show-more:hover { background-color: var(--btn-color); color: var(--btn-bg); }

